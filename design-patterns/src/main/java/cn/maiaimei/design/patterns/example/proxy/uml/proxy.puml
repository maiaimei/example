@startuml
'https://plantuml.com/class-diagram
'https://plantuml.com/zh/class-diagram

ProxySubject --> RealSubject
Client --> Subject

interface Subject {
+ handleRequest()
}

class RealSubject implements Subject {
+ handleRequest()
}

class ProxySubject implements Subject {
- RealSubject realSubject
+ handleRequest()
}

class Client{
}

@enduml
